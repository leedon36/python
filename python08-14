a='Hello Python'
#문자열은 immutable한 자료형이다
print(a.count('o'))
print(a.find('o'))
print(a.index('o'))
print(','.join(a))
print(a.replace('Py','MY'))
print(a.split("o"))
print(a.upper())
print(a.lower())
print(a) #함수를 실행해도  a의 원본 데이터는 변하지않는다.

a=128
print(type(a)) #int=정수 양의 정수,음의 정수,0

num_str ='720'
print(type(num_str))#str = 문자열
num=int(num_str)#자료형 변환
print(type(num))

num=100
num_str=str(num) #형변환 int-> str
print(type(num_str))   # <class 'str'>

letters = 'python'
print(letters[0],letters[2])
