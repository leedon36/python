name1 = "김민수"
age1 = 10
name2 = "이철희"
age2 = 13
print("이름",name1,'나이',age1)
print("이름",name2,'나이',age2)
print("이름: %s 나이: %d" %(name1,age1))
print("이름: %s 나이: %d" %(name2,age2))
#%s=문자열,%d=정수,%f=실수,%c=문자
print('이름:{0} 나이: {0}'.format(name1,age1))
print(f'이름: {name1} 나이: {age1}')
상장주식수 = "5,969,782,550"
a=(상장주식수.replace(',',''))
f=int(a)
print(type(f))
